<a href="/games/<%= game.slug %>">
  <div class="game-card"> 

    <img src="<%= game.imageUrl %>" loading="lazy" decoding="async" alt="<%= game.title %>">

    <p class="game-card-genre">
      <%= game.genre %>
    </p>

    <div class="sep"></div>

    <h1>
      <%= game.title %>
    </h1>

  </div>
</a>